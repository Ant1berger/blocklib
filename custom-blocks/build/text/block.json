{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-blocks/text",
  "version": "0.1.0",
  "title": "Text",
  "category": "text",
  "keywords": [
    "blocklib",
    "text"
  ],
  "description": "A rich text text.",
  "example": {},
  "supports": {
    "html": false,
    "className": false,
    "customClassName": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": ".text"
    },
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "tag": {
      "type": "string",
      "default": "p"
    },
    "selectedColorClass": {
      "type": "string",
      "default": ""
    },
    "selectedFontClass": {
      "type": "string",
      "default": ""
    },
    "manualClasses": {
      "type": "string",
      "default": ""
    },
    "blockStyles": {
      "type": "string",
      "default": ""
    },
    "blockName": {
      "type": "string",
      "default": ""
    },
    "mediaQueries": {
      "type": "array",
      "default": []
    },
    "renderedMediaQueries": {
      "type": "string",
      "default": ""
    },
    "cssColorModifiers": {
      "type": "string",
      "default": ""
    },
    "cssFontModifiers": {
      "type": "string",
      "default": ""
    },
    "blockClasses": {
      "type": "string",
      "default": ""
    },
    "blockStylesTag": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "custom-blocks",
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js"
}