{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-blocks/dropdown",
  "version": "0.1.0",
  "title": "Dropdown",
  "category": "design",
  "keywords": [
    "blocklib",
    "design",
    "dropdown"
  ],
  "description": "An animated component for showing/hiding embeded stuff.",
  "allowedBlocks": [
    "custom-blocks/dropdown-content"
  ],
  "example": {},
  "supports": {
    "html": false,
    "className": false,
    "customClassName": false
  },
  "attributes": {
    "anchor": {
      "type": "string",
      "default": ""
    },
    "persistentID": {
      "type": "string",
      "default": ""
    },
    "tag": {
      "type": "string",
      "default": "div"
    },
    "manualClasses": {
      "type": "string",
      "default": ""
    },
    "blockName": {
      "type": "string",
      "default": ""
    },
    "closeWhenClickOutside": {
      "type": "boolean",
      "default": false
    },
    "openOnHover": {
      "type": "boolean",
      "default": false
    },
    "otherAttributes": {
      "type": "string",
      "default": ""
    },
    "mediaQueries": {
      "type": "array",
      "default": []
    },
    "renderedMediaQueries": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "custom-blocks",
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}