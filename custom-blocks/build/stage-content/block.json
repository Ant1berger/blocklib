{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-blocks/stage-content",
  "version": "0.1.0",
  "title": "Stage content",
  "category": "design",
  "keywords": [
    "blocklib",
    "stage",
    "design"
  ],
  "description": "The mandatory only direct child of the Stage block.",
  "parent": [
    "custom-blocks/stage"
  ],
  "example": {},
  "supports": {
    "html": false,
    "className": false,
    "customClassName": false,
    "lock": false
  },
  "attributes": {
    "anchor": {
      "type": "string",
      "default": ""
    },
    "persistentID": {
      "type": "string",
      "default": ""
    },
    "tag": {
      "type": "string",
      "default": "div"
    },
    "manualClasses": {
      "type": "string",
      "default": ""
    },
    "blockName": {
      "type": "string",
      "default": ""
    },
    "otherAttributes": {
      "type": "string",
      "default": ""
    },
    "mediaQueries": {
      "type": "array",
      "default": []
    },
    "renderedMediaQueries": {
      "type": "string",
      "default": ""
    },
    "lock": {
      "type": "object",
      "default": {
        "move": true,
        "remove": true
      }
    }
  },
  "textdomain": "custom-blocks",
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}