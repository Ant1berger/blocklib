{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-blocks/text",
	"version": "0.1.0",
	"title": "Text",
	"category": "text",
    "keywords": [ "blocklib", "text" ],
	"description": "A rich text text.",
	"example": {},
    "supports": {
        "html": false,
        "className": false,
        "customClassName": false
	},
    "attributes": {
        "anchor": {
            "type": "string",
            "default": ""
        },
		"content": {
			"type": "string",
            "default": ""
		},
        "persistentID": {
            "type": "string",
            "default": ""
        },
        "tag": {
            "type": "string",
            "default": "p"
        },
        "manualClasses": {
            "type": "string",
            "default": ""
        },
        "blockName": {
            "type": "string",
            "default": ""
        },
        "otherAttributes": {
            "type": "string",
            "default": ""
        },
        "mediaQueries": {
            "type": "array",
            "default": []
        },
        "renderedMediaQueries": {
            "type": "string",
            "default": ""
        }
	},
	"textdomain": "custom-blocks",
    "render": "file:./render.php",
	"editorScript": "file:./index.js",
    "editorStyle": "file:./index.css"
}
