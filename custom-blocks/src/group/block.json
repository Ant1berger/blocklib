{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-blocks/group",
	"version": "0.1.0",
	"title": "Group",
	"category": "design",
    "keywords": [ "blocklib", "group", "design" ],
	"description": "An element for grouping other elements.",
	"example": {},
    "supports": {
        "html": false,
        "className": false,
        "customClassName": false
	},
    "attributes": {
        "anchor": {
            "type": "string",
            "default": ""
        },
        "persistentID": {
            "type": "string",
            "default": ""
        },
        "tag": {
            "type": "string",
            "default": "div"
        },
        "selectedBGColorClass": {
            "type": "string",
            "default": ""
        },
        "manualClasses": {
            "type": "string",
            "default": ""
        },
        "blockName": {
            "type": "string",
            "default": ""
        },
        "mediaQueries": {
            "type": "array",
            "default": []
        },
        "renderedMediaQueries": {
            "type": "string",
            "default": ""
        }
	},
	"textdomain": "custom-blocks",
    "render": "file:./render.php",
	"editorScript": "file:./index.js",
    "editorStyle": "file:./index.css"
}
